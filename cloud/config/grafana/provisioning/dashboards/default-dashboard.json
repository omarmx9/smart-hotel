        "showThresholdMarkers": true
      },
      "targets": [{
        "datasource": {"type": "influxdb", "uid": "influxdb"},
        "query": "json_data = from(bucket: \"sensors\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._measurement == \"esp_telemetry\")\n  |> filter(fn: (r) => r._field == \"temperature\")\n  |> last()\n  |> group(columns: [\"room_id\"])\n\nlegacy_data = from(bucket: \"sensors\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._measurement == \"esp_telemetry\")\n  |> filter(fn: (r) => r.sensor_type == \"temperature\")\n  |> filter(fn: (r) => r._field == \"value\")\n  |> last()\n  |> group(columns: [\"room_id\"])\n\nunion(tables: [json_data, legacy_data])",
